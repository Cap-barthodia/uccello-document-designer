{
    "name": "uccello/document-designer",
    "description": "Convert docx files templates into PDF.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri DA COSTA",
            "email": "dimitri.dacosta34@gmail.com"
        }
    ],
    "require": {
        "uccello/uccello": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\DocumentDesigner\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uccello\\DocumentDesigner\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\DocumentDesigner\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
